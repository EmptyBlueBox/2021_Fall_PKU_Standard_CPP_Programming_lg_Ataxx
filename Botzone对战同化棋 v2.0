#include <iostream>
#include <cstring>
#include <cstdio>
#include <algorithm>
#include <ctime>
using namespace std;
int start;//卡时间
int chsBoard[8][8] = { 0 };//黑棋先行，黑棋1，白棋-1
int chess[3][8][8] = { 0 };//黑棋2，白棋1
int tempchsBoard[10][8][8] = { 0 };//黑棋先行，黑棋1，白棋-1
int tempchess[10][3][8][8] = { 0 };//黑棋2，白棋1
int col = 1;
int userCol;
int cnt1 = 0; int cnt2 = 0;
struct better
{
	int x1;
	int y1;
	int x2;
	int y2;
	double value;
}
candidate[30];

bool cmp(better a, better b)
{
	if (a.value > b.value)
		return true;
	else
		return false;
}

//复制棋盘与棋子
void cpy(int step)
{
	int i, j;

	for (i = 0; i <= 7; i++)
	{
		for (j = 0; j <= 7; j++)
		{
			tempchsBoard[step][i][j] = chsBoard[i][j];
			tempchess[step][0][i][j] = chess[0][i][j];
			tempchess[step][2][i][j] = chess[2][i][j];
		}
	}
	return;
}
//重置棋盘与棋子
void rcpy(int step)
{
	int i, j;
	for (i = 0; i <= 7; i++)
	{
		for (j = 0; j <= 7; j++)
		{
			chsBoard[i][j] = tempchsBoard[step][i][j];
			chess[0][i][j] = tempchess[step][0][i][j];
			chess[2][i][j] = tempchess[step][2][i][j];
		}
	}
	return;
}
//输入玩家操作的棋子坐标,目的地，棋子颜色  更改棋盘与棋子的位置记录
void move(int m, int n, int x, int y, int col)
{
	int i, j;

	if (abs(m - x) <= 1 && abs(n - y) <= 1)
	{
		chsBoard[x][y] = col;
		chess[col + 1][x][y] = true;
		chess[col + 1][0][0]++;

		//改变周围棋的颜色
		for (i = x - 1; i <= x + 1; i++)
		{
			for (j = y - 1; j <= y + 1; j++)
			{
				if (i <= 0 || i >= 8 || j <= 0 || j >= 8)
				{
					continue;
				}
				if (i == x && j == y)
					continue;
				if (chsBoard[i][j] == -1 * col)
				{
					chsBoard[i][j] = col;
					chess[col + 1][i][j] = true;
					chess[1 + col][0][0]++;
					chess[1 - col][i][j] = false;
					chess[1 - col][0][0]--;
				}
			}
		}
	}
	else
	{
		chsBoard[x][y] = col;
		chsBoard[m][n] = 0;
		chess[col + 1][x][y] = true;
		chess[col + 1][0][0]++;
		chess[col + 1][m][n] = false;
		chess[col + 1][0][0]--;

		//改变周围棋的颜色
		for (i = x - 1; i <= x + 1; i++)
		{
			for (j = y - 1; j <= y + 1; j++)
			{
				if (i <= 0 || i >= 8 || j <= 0 || j >= 8)
				{
					continue;
				}
				if (i == x && j == y)
					continue;
				if (chsBoard[i][j] == -1 * col)
				{
					chsBoard[i][j] = col;
					chess[1 + col][i][j] = true;
					chess[1 + col][0][0]++;
					chess[1 - col][i][j] = false;
					chess[1 - col][0][0]--;
				}
			}
		}
	}
	return;
}
